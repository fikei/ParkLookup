# Default environment variables for Fastlane
# Copy this to .env and fill in your values
# DO NOT commit .env file with real credentials!

# Apple Developer Account
APPLE_ID=your-apple-id@example.com
TEAM_ID=YOUR_TEAM_ID
ITC_TEAM_ID=YOUR_ITC_TEAM_ID

# App Configuration
APP_IDENTIFIER=com.yourcompany.sfparkingzonefinder

# Provisioning
PROVISIONING_PROFILE_SPECIFIER=match AppStore com.yourcompany.sfparkingzonefinder

# App Store Connect API Key (recommended for CI/CD)
APP_STORE_CONNECT_API_KEY_PATH=./fastlane/api_key.json

# Git Configuration
ALLOWED_BRANCH=main
SKIP_GIT_CHECK=false
SKIP_BRANCH_CHECK=false
SKIP_GIT_PUSH=false
